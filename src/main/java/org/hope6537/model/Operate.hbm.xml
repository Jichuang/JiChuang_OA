<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.hope6537.model.Operate" table="Operate_Database">
		<id name="oid" type="integer">
			<column name="oid"></column>
			<generator class="increment"></generator>
		</id>
		<property name="odes" type="string">
			<column name="odes"></column>
		</property>
		<property name="oinfo" type="string">
			<column name="oinfo"></column>
		</property>
		<property name="ostatus" type="string">
			<column name="ostatus"></column>
		</property>
		<set name="oroles" table="operate_post_Database">
			<key column="oid"></key>
			<many-to-many class="org.hope6537.model.Role" column="rid"></many-to-many>
		</set>
	</class>

</hibernate-mapping>
