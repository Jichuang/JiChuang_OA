<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.jichuang.hope6537.base.model.Post" table="Post_Database">
		<id name="pid" type="integer">
			<column name="pid"></column>
			<generator class="increment"></generator>
		</id>
		<property name="pdes" type="string">
			<column name="pdes"></column>
		</property>
		<property name="pstatus" type="string">
			<column name="pstatus"></column>
		</property>
		<set name="members" table="post_member_Database">
			<key column="pid"></key>
			<many-to-many class="org.jichuang.hope6537.base.model.Member" column="mid"></many-to-many>
		</set>

		<set name="roles" table="role_post_Database">
			<key column="pid"></key>
			<many-to-many class="org.jichuang.hope6537.base.model.Role" column="rid"></many-to-many>
		</set>
	</class>

</hibernate-mapping>
