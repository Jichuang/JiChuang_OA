<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.hope6537.model.ProjectText" table="ProjectText_DataBase">
		<id name="ptid" type="int">
			<column name="ptid"></column>
			<generator class="increment"></generator>
		</id>
		<property name="pttitle" type="string">
			<column name="pttitle"></column>
		</property>
		<property name="ptinfo" type="string" length="500">
			<column name="pttinfo"></column>
		</property>
		<property name="ptstatus" type="string" length="50">
			<column name="ptstatus"></column>
		</property>
		<property name="ptcont" type="string" length="500">
			<column name="ptcont"></column>
		</property>
		<many-to-one name="pttid" cascade="save-update"
			class="org.hope6537.model.Team" column="pttid"></many-to-one>
		<many-to-one name="ptmid" cascade="save-update"
			class="org.hope6537.model.Member" column="ptmid"></many-to-one>
	</class>

</hibernate-mapping>
